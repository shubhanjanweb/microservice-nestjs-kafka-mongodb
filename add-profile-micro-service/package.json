    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:aws:build": "docker build -t 042134106060.dkr.ecr.ap-south-1.amazonaws.com/add-profile-microservice .",
    "docker:aws:commit": "docker push 042134106060.dkr.ecr.ap-south-1.amazonaws.com/add-profile-microservice",
    "docker:build": "docker build -t shubhanjanweb/iiht-fse4-sta-add-profile-microservice-docker-image:1.0 .",
    "docker:run": "docker run --env MONGO_INITDB_ROOT_USERNAME=\"iihtAdmin\" --env MONGO_INITDB_ROOT_PASSWORD=\"iihtpassword2022\" --env MONGO_INITDB_DATABASE=\"SkillTrackerDB\" --env KAFKA_BROKER_URL=\"kafka: 9092\" --env MONGODB_URL=\"mongo:27017\" --name iiht-skill-tracker-add-profile-microservice -d shubhanjanweb/iiht-fse4-sta-add-profile-microservice-docker-image:1.0",
    "docker:commit": "docker push shubhanjanweb/iiht-fse4-sta-add-profile-microservice-docker-image:1.0"
